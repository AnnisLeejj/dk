<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.annis.dk">
    
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="${app_name}"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".ui.emergency_contact.EmergencyContactActivity">
        </activity>
        <activity android:name=".ui.mine.bankmanage.BankManageActivity">
        </activity>
        <activity android:name=".ui.mine.mineLoans.MyLoansActivity">
